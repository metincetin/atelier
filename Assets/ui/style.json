{
    ".h1":{
        "font-size":48
    },
    ".h3":{
        "font-size": 22
    },
    ".text-center":{
        "alignment":"middle-center"
    },
    ".fullRect":{
        "layout.anchorsMin.x": 0,
        "layout.anchorsMin.y": 0,
        "layout.anchorsMax.x": 1,
        "layout.anchorsMax.y": 1
    },
    "#mainContainer":{
        "child-start": "middle"
    },
    "#mainRoot":{
        "child-start": "middle"
    },
    "#mainRoot *":{
        "layout.vertical-alignment": "center"
    },
    "#buttonContainer *":{
        "layout.vertical-alignment": "center"
    },
    "Button.positive":{
        "background-color":"#18b840",
        "font-size": 22,
        "states":{
            "hovered":{
                "background-color":"#a4b420"
            },
            "pressed":{
                "background-color":"#ff0000"
            }
        }
    },
    "VerticalBoxContainer":{
        "spacing": 4
    },
    "#version":{
        "font-size": 12,
        "layout.anchorsMin.x": 1,
        "layout.anchorsMin.y": 1,
        "layout.anchorsMax.x": 1,
        "layout.anchorsMax.y": 1,
        "pivot.x": 1,
        "pivot.y": 1
    },
    "#root":{
        "layout.anchorsMin.x": 0,
        "layout.anchorsMin.y": 0,
        "layout.anchorsMax.x": 1,
        "layout.anchorsMax.y": 1
    },
    ".collapsed":{
        "visibility":"collapsed"
    }
}